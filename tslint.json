{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "semicolon": [true, "never"],
        "quotemark": [true, "single", "avoid-escape", "avoid-template"],
        "arrow-parens": [true, "ban-single-arg-parens"],
        "space-before-function-paren": [true, "always"],
        "no-empty-interface": [false]
    },
    "rulesDirectory": []
}
